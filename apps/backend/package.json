{
  "name": "@myorok/backend",
  "version": "1.0.0",
  "description": "Backend server for Myorok Kakao OAuth authentication",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc && cp -r src/views dist/views",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "kakao",
    "oauth",
    "authentication",
    "express"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
